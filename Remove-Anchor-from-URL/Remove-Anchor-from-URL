public static class Kata
{
  public static string RemoveUrlAnchor(string url)
        {
            string urlNoAnchor = "";
            foreach (var character in url)
            {
                if (character == '#')
                {
                    break;
                }
                else
                {
                    urlNoAnchor += character;
                }
            }
            return urlNoAnchor;
        }
}
